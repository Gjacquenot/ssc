@PACKAGE_INIT@

set_and_check(ssc_INCLUDE_DIRS "@ssc_INCLUDE_DIRS@")

include ( "${CMAKE_CURRENT_LIST_DIR}/sscTargets.cmake" )

set(ssc_VERSION @ssc_MAJOR_VERSION@.@ssc_MINOR_VERSION@.@ssc_PATCH_VERSION@)
set(PACKAGE_VERSION "@ssc_VERSION@")

if(${PACKAGE_FIND_VERSION_MAJOR})
    if(NOT @ssc_MAJOR_VERSION@ MATCHES ${PACKAGE_FIND_VERSION_MAJOR})
        set(PACKAGE_VERSION_COMPATIBLE FALSE)
    else()
        if(@ssc_MINOR_VERSION@ LESS ${PACKAGE_FIND_VERSION_MINOR})
            set(PACKAGE_VERSION_COMPATIBLE FALSE)
        else()
            set(PACKAGE_VERSION_COMPATIBLE TRUE)
        endif()
        if (${ssc_VERSION} VERSION_EQUAL "${PACKAGE_FIND_VERSION}")
            set(PACKAGE_VERSION_EXACT TRUE)
        endif()
    endif()
else()
    set(PACKAGE_VERSION_COMPATIBLE TRUE)
    set(PACKAGE_VERSION_EXACT FALSE)
endif()
